{
  "name": "fjs-utils",
  "author": "Jason Matthews <jason.matthews.dev@gmail.com",
  "version": "1.7.1",
  "description": "",
  "main": "dist",
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "npx babel ./src -d ./dist",
    "copyfiles": "npm run copy:package && npm run copy:readme",
    "copy:package": "cp -rf package.json dist",
    "copy:readme": "cp -rf Readme.md dist",
    "lint": "npx eslint ./src ./tests",
    "semantic-release": "semantic-release",
    "test": "jest --coverage"
  },
  "keywords": [
    "utility",
    "functional",
    "lib",
    "js",
    "javascript",
    "fjs"
  ],
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-preset-minify": "^0.5.1",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "semantic-release": "^17.1.1",
    "sinon": "^9.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JasonMatthewsDev/fjsutils.git"
  }
}
